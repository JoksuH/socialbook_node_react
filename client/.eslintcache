aveComment/LeaveComment'\nimport { useState, useEffect } from 'react'\n\nconst StyledGrid = styled(Grid)({\n    marginBottom: '50px',\n})\n\nconst Post = ({ post }) => {\n    const [Liked, SetLiked] = useState(false)\n    const [Comments, SetComments] = useState([])\n    const [Comment, SetComment] = useState('')\n\n    useEffect(() => {\n        SetComments(post.Comments)\n    }, [])\n\n    const onTextChange = (event) => SetComment(event.target.value)\n\n    const handleLikeClick = () => {\n        fetch(`http://localhost:5000/likes/${post._id}`, {\n            method: 'PUT',\n            headers: {\n                Accept: 'application/json',\n                'Content-Type': 'application/json',\n                Authorization: `Bearer ${localStorage.getItem('JWTtoken')}`,\n            },\n        }).then((response) => SetLiked(true))\n    }\n\n    const handleLeaveComment = (event) => {\n        fetch(`http://localhost:5000/comments/`, {\n            method: 'POST',\n            headers: {\n                Accept: 'application/json',\n                'Content-Type': 'application/json',\n                Authorization: `Bearer ${localStorage.getItem('JWTtoken')}`,\n            },\n            body: JSON.stringify({\n                Comment: Comment,\n                ParentPost: post._id,\n            }),\n        })\n            .then((response) => response.json())\n            .then((json) => {\n                SetComments(Comments.concat(json))\n\n                //Reset comment field's text value\n                event.target.firstElementChild.lastChild.firstChild.value = ''\n            })\n\n        event.preventDefault()\n    }\n\n    return (\n        <StyledGrid>\n            <AvatarInfo data={post} />\n\n            <Grid item xs={12}>\n                <Typography variant=\"body1\">{post.Body}</Typography>\n            </Grid>\n            <LikeBar likeClicked={handleLikeClick} isLiked={Liked} />\n            <Divider variant=\"middle\" />\n            <LikesAndShares Postdata={post} liked={Liked} />\n            <Divider />\n            <CommentListing Comments={Comments} />\n            <LeaveComment\n                Post={post}\n                handleLeaveComment={handleLeaveComment}\n                handleTextChange={onTextChange}\n            />\n        </StyledGrid>\n    )\n}\n\nexport default Post\n","/home/joksu/socialbook/client/src/Components/Post/AvatarInfo/AvatarInfo.js",[],"/home/joksu/socialbook/client/src/Components/Post/LikeBar/LikeBar.js",[],"/home/joksu/socialbook/client/src/Components/Post/LikesAndShares/LikesAndShares.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/Rightsidebar.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/FriendsSuggestions/FriendsSuggestions.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/FriendsSuggestions/Suggestion.js",[],"/home/joksu/socialbook/client/src/Components/Post/LeaveComment/LeaveComment.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/Comments.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/Comment.js",[],"/home/joksu/socialbook/client/src/Components/Post/AvatarInfo/CommentAvatarInfo.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/CommentName.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/CommentTime.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/LoadMoreComments.js",[],"/home/joksu/socialbook/client/src/Components/Routes/PublicRoute.js",[],"/home/joksu/socialbook/client/src/Components/Routes/PrivateRoute.js",[],"/home/joksu/socialbook/client/src/Components/Home/HomeView.js",[],"/home/joksu/socialbook/client/src/Components/ManageFriends/ManageFriends.js",[],"/home/joksu/socialbook/client/src/Components/ManageFriends/Friend.js",[],"/home/joksu/socialbook/client/src/Components/Navbar/Navbar.js",[],"/home/joksu/socialbook/client/src/Components/Navbar/SearchField/SearchField.js",[],"/home/joksu/socialbook/client/src/Com