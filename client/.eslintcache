ox>\n        </Box>\n    )\n}\n\nconst WantRequestFriend = connect(mapStateToProps)(RequestFriend)\n\nexport default WantRequestFriend\n","/home/joksu/socialbook/client/src/Components/Messenger/MessengerView.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/Messenger.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/OpenConversations.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/CurrentConversation.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/StartNewConversation.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/FriendList/FriendList.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/FriendList/Friend.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/ChatBox.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/ChatWriteBox.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/OpenConversation.js",[],"/home/joksu/socialbook/client/src/Components/Messenger/ChatMessage.js",[],"/home/joksu/socialbook/client/src/Components/AddPost/AddPost.js",["234"],{"ruleId":"235","replacedBy":"236"},{"ruleId":"237","replacedBy":"238"},{"ruleId":"239","severity":1,"message":"240","line":41,"column":8,"nodeType":"241","endLine":41,"endColumn":10,"suggestions":"242"},{"ruleId":"243","severity":1,"message":"244","line":7,"column":7,"nodeType":"245","messageId":"246","endLine":7,"endColumn":19},{"ruleId":"239","severity":1,"message":"247","line":27,"column":8,"nodeType":"241","endLine":27,"endColumn":18,"suggestions":"248"},{"ruleId":"239","severity":1,"message":"249","line":23,"column":8,"nodeType":"241","endLine":23,"endColumn":10,"suggestions":"250"},{"ruleId":"239","severity":1,"message":"251","line":55,"column":8,"nodeType":"241","endLine":55,"endColumn":14,"suggestions":"252"},{"ruleId":"243","severity":1,"message":"253","line":2,"column":8,"nodeType":"245","messageId":"246","endLine":2,"endColumn":17},"no-native-reassign",["254"],"no-negated-in-lhs",["255"],"react-ho