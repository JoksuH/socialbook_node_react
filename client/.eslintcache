m '@material-ui/core/styles'\nimport CloudUploadIcon from '@material-ui/icons/CloudUpload'\n\nconst StyledButton = styled(Button)({\nmarginLeft: '45%'\n\n})\n\n\nconst Writepad = ({OnPost, OnTextChange, Textvalue}) => {\n    \n\n    return (\n        <div className=\"articleWriter\">\n            <form onSubmit={OnPost}>\n                <TextField\n                    multiline={false}\n                    rows=\"3\"\n                    id=\"ArticleBodyText\"\n                    fullWidth={true}\n                    variant=\"outlined\"\n                    label=\"Post\"\n                    value={Textvalue}\n                    onChange={OnTextChange}\n                />\n                <br />\n                <br />\n                <div className=\"buttonRow\" >\n                    <Button\n                        variant=\"contained\"\n                        color=\"primary\"\n                        type=\"submit\"\n                        startIcon={<CloudUploadIcon />}\n                    >\n                        {' '}\n                        Post!\n                    </Button>\n                </div>\n            </form>\n    \n        </div>\n    )\n}\n\nexport default Writepad\n","/home/joksu/socialbook/client/src/Components/Utils.js",[],"/home/joksu/socialbook/client/src/Components/MainContent/Maincontent.js",["228"],"/home/joksu/socialbook/client/src/Components/Leftsidebar/Leftsidebar.js",[],"/home/joksu/socialbook/client/src/Components/Leftsidebar/AvatarField/AvatarField.js",[],"/home/joksu/socialbook/client/src/Components/Leftsidebar/SidebarLinks/SidebarLinks.js",[],"/home/joksu/socialbook/client/src/Components/Post/Post.js",["229"],"import Grid from '@material-ui/core/Grid'\nimport Divider from '@material-ui/core/Divider'\nimport Typography from '@material-ui/core/Typography'\nimport { styled } from '@material-ui/core/styles'\nimport AvatarInfo from './AvatarInfo/AvatarInfo'\nimport LikeBar from './LikeBar/LikeBar'\nimport LikesAndShares from './LikesAndShares/LikesAndShares'\nimport CommentListing from './Comments/Comments'\nimport LeaveComment from './LeaveComment/LeaveComment'\nimport { useState, useEffect } from 'react'\n\nconst StyledGrid = styled(Grid)({\n    marginBottom: '50px',\n})\n\nconst Post = ({ post }) => {\n    const [Liked, SetLiked] = useState(false)\n    const [Comments, SetComments] = useState([])\n    const [Comment, SetComment] = useState('')\n\n    useEffect(() => {\n        SetComments(post.Comments)\n    }, [])\n\n    const onTextChange = (event) => SetComment(event.target.value)\n\n    const handleLikeClick = () => {\n        fetch(`http://localhost:5000/likes/${post._id}`, {\n            method: 'PUT',\n            headers: {\n                Accept: 'application/json',\n                'Content-Type': 'application/json',\n                Authorization: `Bearer ${localStorage.getItem('JWTtoken')}`,\n            },\n        }).then((response) => SetLiked(true))\n    }\n\n    const handleLeaveComment = (event) => {\n        fetch(`http://localhost:5000/comments/`, {\n            method: 'POST',\n            headers: {\n                Accept: 'application/json',\n                'Content-Type': 'application/json',\n                Authorization: `Bearer ${localStorage.getItem('JWTtoken')}`,\n            },\n            body: JSON.stringify({\n                Comment: Comment,\n                ParentPost: post._id,\n            }),\n        })\n            .then((response) => response.json())\n            .then((json) => {\n                SetComments(Comments.concat(json))\n\n                //Reset comment field's text value\n                event.target.firstElementChild.lastChild.firstChild.value = ''\n            })\n\n        event.preventDefault()\n    }\n\n    return (\n        <StyledGrid>\n            <AvatarInfo data={post} />\n\n            <Grid item xs={12}>\n                <Typography variant=\"body1\">{post.Body}</Typography>\n            </Grid>\n            <LikeBar likeClicked={handleLikeClick} isLiked={Liked} />\n            <Divider variant=\"middle\" />\n            <LikesAndShares Postdata={post} liked={Liked} />\n            <Divider />\n            <CommentListing Comments={Comments} />\n            <LeaveComment\n                Post={post}\n                handleLeaveComment={handleLeaveComment}\n                handleTextChange={onTextChange}\n            />\n        </StyledGrid>\n    )\n}\n\nexport default Post\n","/home/joksu/socialbook/client/src/Components/Post/AvatarInfo/AvatarInfo.js",[],"/home/joksu/socialbook/client/src/Components/Post/LikeBar/LikeBar.js",[],"/home/joksu/socialbook/client/src/Components/Post/LikesAndShares/LikesAndShares.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/Rightsidebar.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/FriendsSuggestions/FriendsSuggestions.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/FriendsSuggestions/Suggestion.js",[],"/home/joksu/socialbook/client/src/Components/Post/LeaveComment/LeaveComment.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/Comments.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/Comment.js",[],"/home/joksu/socialbook/client/src/Components/Post/AvatarInfo/CommentAvatarInfo.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/CommentName.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/CommentTime.js",[],"/home/joksu/socialbook/client/src/Components/Post/Comments/LoadMoreComments.js",[],"/home/joksu/socialbook/client/src/Components/Routes/PublicRoute.js",[],"/home/joksu/socialbook/client/src/Components/Routes/PrivateRoute.js",[],"/home/joksu/socialbook/client/src/Components/Home/HomeView.js",[],"/home/joksu/socialbook/client/src/Components/ManageFriends/ManageFriends.js",[],"/home/joksu/socialbook/client/src/Components/ManageFriends/Friend.js",[],"/home/joksu/socialbook/client/src/Components/Navbar/Navbar.js",[],"/