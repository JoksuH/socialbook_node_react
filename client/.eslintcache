Bearer ${localStorage.getItem('JWTtoken')}`,\n            },\n        }).then((response) => SetLiked(true))\n    }\n\n    const handleLeaveComment = (event) => {\n        fetch(`http://localhost:5000/comments/`, {\n            method: 'POST',\n            headers: {\n                Accept: 'application/json',\n                'Content-Type': 'application/json',\n                Authorization: `Bearer ${localStorage.getItem('JWTtoken')}`,\n            },\n            body: JSON.stringify({\n                Comment: Comment,\n                ParentPost: post._id,\n            }),\n        })\n            .then((response) => response.json())\n            .then((json) => {\n                SetComments(Comments.concat(json))\n\n                //Reset comment field's text value\n                event.target.firstElementChild.lastChild.firstChild.value = ''\n            })\n\n        event.preventDefault()\n    }\n\n    return (\n        <StyledGrid>\n            <AvatarInfo data={post} />\n\n            <Grid item xs={12}>\n                <Typography variant=\"body1\">{post.Body}</Typography>\n            </Grid>\n            <LikeBar likeClicked={handleLikeClick} isLiked={Liked} />\n            <Divider variant=\"middle\" />\n            <LikesAndShares Postdata={post} liked={Liked} />\n            <Divider />\n            <CommentListing Comments={Comments} />\n            <LeaveComment\n                Post={post}\n                handleLeaveComment={handleLeaveComment}\n                handleTextChange={onTextChange}\n            />\n        </StyledGrid>\n    )\n}\n\nexport default Post\n","/home/joksu/socialbook/client/src/Components/Post/AvatarInfo/AvatarInfo.js",[],"/home/joksu/socialbook/client/src/Components/Post/LikeBar/LikeBar.js",[],"/home/joksu/socialbook/client/src/Components/Post/LikesAndShares/LikesAndShares.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/Rightsidebar.js",[],"/home/joksu/socialbook/client/src/Components/Rightsidebar/F